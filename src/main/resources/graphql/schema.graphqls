type Query {
    users: [User!]!
    user(id: ID!): User!
}

type Mutation {
    createUser(name: String!, email: String): User!
    updateProfilePicture(userId: ID!,profilePictureUrl: String): User!
    deleteUser(userId: ID!): Boolean!
}

type User {
    id: ID!,
    name: String!,
    email: String!,
    profilePictureUrl: String
}